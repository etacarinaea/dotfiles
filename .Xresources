Xft.dpi:                96
Xft.antialias:          false
Xft.hinting:            true
Xft.rgba:               rgb
Xft.autohint:           false
Xft.hintstyle:          hintfull
Xft.lcdfilter:          lcddefault

URxvt*geometry:         83x21
URxvt*scrollBar:        false


URxvt*foreground:       #aaaaaa
URxvt*background:       #22272e
URxvt*cursorColor:      #aaaaaa

! black
URxvt*color0:           #2b323b
URxvt*color8:           #272727

! red
URxvt*color1:           #7e1f41
URxvt*color9:           #7e1f41

! green
URxvt*color2:           #5f6b5b
URxvt*color10:          #5f6b5b

! yellow
URxvt*color3:           #d2c495
URxvt*color11:          #d2c495

! blue
URxvt*color4:           #2f4d60
URxvt*color12:          #2f4d60

! magenta
URxvt*color5:           #454757
URxvt*color13:          #454757

! cyan
URxvt*color6:           #7e1f41
URxvt*color14:          #7e1f41

! white
URxvt*color7:           #7e1f41
URxvt*color15:          #7e1f41


URxvt*font:             xft:tewi:pixelsize=10
URxvt*boldFont:         xft:tewi:pixelsize=10


URxvt*internalBorder:   0


URxvt*perl-ext-common:  default,matcher,clipboard
URxvt*perl-ext:         default,matcher,clipboard
URxvt*url-launcher:     /usr/bin/xdg-open
URxvt*matcher.button:   1

