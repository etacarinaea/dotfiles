*geometry:          83x21
*scrollBar:         false


*foreground:        #f8f8f2
*background:        #282a36
*cursorColor:       #f8f8f2

! black
*color0:            #3c3e4d
*color8:            #454757

! red
*color1:            #ff5555
*color9:            #ff5555

! green
*color2:            #5f6b5b
*color10:           #5f6b5b

! yellow
*color3:            #ffffaf
*color11:           #ffffaf

! blue
*color4:            #bd93f9
*color12:           #bd93f9

! magenta
*color5:            #bd93f9
*color13:           #bd93f9

! cyan
*color6:            #61d8c2
*color14:           #61d8c2

! white
*color7:            #7e1f41
*color15:           #7e1f41

*color66:           #100b08


*font:              xft:gohufont:pixelsize=14, xft:Anonymous Pro for Powerline:pixelsize=14
*boldFont:          xft:gohufont:pixelsize=14, xft:Anonymous Pro for Powerline:pixelsize=14


*letterSpace:       1
*lineSpace:         4

*internalBorder:    35


URxvt*perl-ext-common:      default,url-select,keyboard-select,selection-to-clipboard
URxvt*url-select.launcher:  /usr/bin/xdg-open
URxvt*url-select.underline: true

